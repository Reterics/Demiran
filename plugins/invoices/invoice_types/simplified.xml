<?xml version="1.0" encoding="UTF-8"?>
<InvoiceData xmlns="http://schemas.nav.gov.hu/OSA/3.0/data" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.nav.gov.hu/OSA/3.0/data invoiceData.xsd" xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common" xmlns:base="http://schemas.nav.gov.hu/OSA/3.0/base">
	<invoiceNumber>{{invoiceNumber}}</invoiceNumber>
	<invoiceIssueDate>{{invoiceIssueDate}}</invoiceIssueDate>
	<completenessIndicator>false</completenessIndicator>
	<invoiceMain>
		<invoice>
			<invoiceHead>
				<supplierInfo>
					<supplierTaxNumber>
						<base:taxpayerId>{{supplierTaxpayerId}}</base:taxpayerId>
						<base:vatCode>{{supplierVatCode}}</base:vatCode>
						<base:countyCode>{{supplierCountyCode}}</base:countyCode>
					</supplierTaxNumber>
					<supplierName>{{supplierName}}</supplierName>
					<supplierAddress>
						<base:detailedAddress>
							<base:countryCode>HU</base:countryCode>
							<base:postalCode>{{supplierPostCode}}</base:postalCode>
							<base:city>{{supplierTown}}</base:city>
							<base:streetName>{{supplierStreetName}}</base:streetName>
							<base:publicPlaceCategory>{{supplierStreet}}</base:publicPlaceCategory>
							<base:number>{{supplierAddress}}</base:number>
						</base:detailedAddress>
					</supplierAddress>
					<supplierBankAccountNumber>{{supplierBankAccountNumber}}</supplierBankAccountNumber>
				</supplierInfo>
				<customerInfo>
					<customerVatStatus>{{customerVatStatus}}</customerVatStatus>
					{#customerTaxNumber}}
						<customerVatData>
							<customerTaxNumber>
								<base:taxpayerId>{{customerTaxpayerId}}</base:taxpayerId>
								<base:vatCode>{{customerVatCode}}</base:vatCode>
								<base:countyCode>{{customerCountyCode}}</base:countyCode>
							</customerTaxNumber>
						</customerVatData>
					{/#customerTaxNumber}}
					<customerName>{{customerName}}</customerName>
					<customerAddress>
						<base:detailedAddress>
							<base:countryCode>HU</base:countryCode>
							<base:postalCode>{{customerPostCode}}</base:postalCode>
							<base:city>{{customerTown}}</base:city>
							<base:streetName>{{customerStreetName}}</base:streetName>
							<base:publicPlaceCategory>{{customerStreet}}</base:publicPlaceCategory>
							<base:number>{{customerAddress}}</base:number>
						</base:detailedAddress>
					</customerAddress>
				</customerInfo>
				<invoiceDetail>
					<invoiceCategory>{{invoiceCategory}}</invoiceCategory>
					<invoiceDeliveryDate>{{invoiceDeliveryDate}}</invoiceDeliveryDate>
					<currencyCode>{{currencyCode}}</currencyCode>
					<exchangeRate>1</exchangeRate>
					<paymentMethod>{{paymentMethod}}</paymentMethod>
					<cashAccountingIndicator>true</cashAccountingIndicator>
					<invoiceAppearance>{{invoiceAppearance}}</invoiceAppearance>
				</invoiceDetail>
			</invoiceHead>
			<invoiceLines>
				<mergedItemIndicator>false</mergedItemIndicator>
				{>invoiceLines}}
				<line>
					<lineNumber>{{+id}}</lineNumber>
					<productCodes>
						<productCode>
							<productCodeCategory>{{+productCodeCategory}}</productCodeCategory>
							<productCodeValue>{{+productCodeValue}}</productCodeValue>
						</productCode>
					</productCodes>
					<lineExpressionIndicator>true</lineExpressionIndicator>
					<lineNatureIndicator>{{+lineNatureIndicator}}</lineNatureIndicator>
					<lineDescription>{{+lineDescription}}</lineDescription>
					<quantity>{{+quantity}}</quantity>
					<unitOfMeasure>{{+unitOfMeasure}}</unitOfMeasure>
					<unitPrice>{{+unitPrice}}</unitPrice>
					<unitPriceHUF>{{+unitPrice}}</unitPriceHUF>
					<lineAmountsSimplified>
						<lineVatRate>
							<vatContent>{{+lineVatRate}}</vatContent>
						</lineVatRate>
						<lineGrossAmountSimplified>{{+lineGrossAmountData}}</lineGrossAmountSimplified>
						<lineGrossAmountSimplifiedHUF>{{+lineGrossAmountData}}</lineGrossAmountSimplifiedHUF>
					</lineAmountsSimplified>
				</line>
				{/>invoiceLines}}
			</invoiceLines>
			<invoiceSummary>
				<summarySimplified>
					<vatRate>
						<vatContent>0.2126</vatContent>
					</vatRate>
					<vatContentGrossAmount>24000</vatContentGrossAmount>
					<vatContentGrossAmountHUF>24000</vatContentGrossAmountHUF>
				</summarySimplified>
				<summarySimplified>
					<vatRate>
						<vatContent>0.0476</vatContent>
					</vatRate>
					<vatContentGrossAmount>4000</vatContentGrossAmount>
					<vatContentGrossAmountHUF>4000</vatContentGrossAmountHUF>
				</summarySimplified>
				<summarySimplified>
					<vatRate>
						<vatExemption>
							<case>TAM</case>
							<reason>Adómentes ÁFA tv. 86.§ (1)</reason>
						</vatExemption>					
					</vatRate>
					<vatContentGrossAmount>5000</vatContentGrossAmount>
					<vatContentGrossAmountHUF>5000</vatContentGrossAmountHUF>
				</summarySimplified>
				<summaryGrossData>
					<invoiceGrossAmount>33000</invoiceGrossAmount>
					<invoiceGrossAmountHUF>33000</invoiceGrossAmountHUF>
				</summaryGrossData>
			</invoiceSummary>
		</invoice>
	</invoiceMain>
</InvoiceData>
